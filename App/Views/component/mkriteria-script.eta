  <script>
    setTimeout(() => {
      document.getElementById("alert").remove()
    }, 1200)

    window.onscroll = function() {
      var myDiv = document.getElementById('tbl-ch');
      var myParent = document.getElementById('tbl-p');
      if (myParent.clientHeight-0 > myParent.clientWidth-0) {

        let totalScroll = window.scrollY;

        myDiv.style.marginTop = totalScroll + "px";
        console.log("Total panjang scroll: " + totalScroll + " piksel");
      }
    };
  </script>